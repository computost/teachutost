{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": ["output-file.txt", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prebuild": {},
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {}
  },
  "globalPassThroughEnv": ["PROGRAMFILES", "PROGRAMFILES(X86)", "USERPROFILE"]
}
